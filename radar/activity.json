{
  "name": "radardata",
  "title": "Process Radar Data",
  "version": "0.0.1",
  "type": "flogo:activity",
  "author":"Hugo Cruz",

  "display": {
    "category": "Hugo",
    "visible": true,
    "smallIcon": "radar.png",
    "description": "Process Radar Data send by Hugo Cruz Go Lang Publisher"
 },

  "ref": "github.com/hugomcruz/flogorepo/radar",

  "inputs": [
    {
      "name": "payload",
      "type": "any",
      "required": true
    }
  ],
  "outputs": [
    {
      "name": "msgType",
      "type": "string"
    },
    {
      "name": "timestamp",
      "type": "integer"
    },
    {
      "name": "icaoHexCode",
      "type": "string"
    },
    {
      "name": "callsign",
      "type": "string"
    },
    {
      "name": "altitude",
      "type": "integer"
    },
    {
      "name": "latitude",
      "type": "double"
    },
    {
      "name": "longitude",
      "type": "double"
    },
    {
      "name": "onGround",
      "type": "integer"
    },
    {
      "name": "groundSpeed",
      "type": "double"
    },
    {
      "name": "track",
      "type": "double"
    },
    {
      "name": "verticalRate",
      "type": "integer"
    }
  ]
}
