{
  "name": "radardata",
  "type": "flogo:activity",
  "ref": "github.com/hugomcruz/flogorepo/radar",
  "version": "0.0.1",
  "title": "Process Radar Data",
  "description": "Process Radar Data send by Hugo Cruz Go Lang Publisher",
  "homepage": "https://github.com/hugomcruz/flogo-repo/radar",
  "settings": [
    {
      "name": "mySetting",
      "type": "string",
      "required": true
    }
  ],
  "input": [
    {
      "name": "payload",
      "type": "object",
      "required": true
    }
  ],
  "output": [
    {
      "name": "msgType",
      "type": "string"
    },
    {
      "name": "timestamp",
      "type": "int64"
    },
    {
      "name": "icaoHexCode",
      "type": "string"
    },
    {
      "name": "callsign",
      "type": "string"
    },
    {
      "name": "altitude",
      "type": "int32"
    },
    {
      "name": "latitude",
      "type": "float32"
    },
    {
      "name": "longitude",
      "type": "float32"
    },
    {
      "name": "onGround",
      "type": "int32"
    },
    {
      "name": "groundSpeed",
      "type": "float32"
    },
    {
      "name": "track",
      "type": "float32"
    },
    {
      "name": "verticalRate",
      "type": "int32"
    }
  ]
}
